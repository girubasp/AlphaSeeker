{
  "name": "alpha-seeker",
  "version": "1.0.0",
  "description": "alpha seeker trading app ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/girubasp/AlphaSeeker.git"
  },
  "keywords": [
    "react",
    "alpha",
    "trader"
  ],
  "author": "girubasp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/girubasp/AlphaSeeker/issues"
  },
  "homepage": "https://github.com/girubasp/AlphaSeeker#readme",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run dev:local",
    "lint": "eslint --ext .js --ext .jsx src",
    "dev:local": "npm run dev -- --endpoint local",
    "dev:docker": "npm run dev -- --endpoint docker",
    "dev:cloud": "npm run dev -- --endpoint demo",
    "dev:openfin:local": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:local",
    "dev:openfin:cloud": "node ./node_modules/npm-run-all/bin/npm-run-all --parallel openfin dev:cloud",
    "dev": "webpack-dev-server --progress --debug --config webpack.config.js --display-error-details",
    "test": "karma start ./karma.conf.js",
    "test:dev": "npm test -- --watch",
    "deploy": "webpack --display-error-details",
    "deploy:prod": "SET NODE_ENV=production & webpack",
    "openfin": "openfin -c config/openfin/local.app.json -l",
    "workbench": "start-storybook -p 9001"
  },
  "dependencies": {
    "animate.css": "3.5.2",
    "autobahn": "0.9.9",
    "classnames": "2.2.5",
    "d3": "3.5.17",
    "esp-js": "1.3.0",
    "esp-js-react": "0.1.6",
    "fixed-data-table": "0.6.3",
    "font-awesome": "4.7.0",
    "lodash": "3.10.1",
    "moment": "2.18.1",
    "numeral": "1.5.6",
    "nvd3": "1.8.5",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-nvd3": "0.5.7",
    "react-sizeme": "2.3.1",
    "react-sparklines": "1.6.0",
    "rx": "4.1.0",
    "ua-parser-js": "0.7.12",
    "ws": "0.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "1.1.1",
    "copy-webpack-plugin": "0.3.3",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "1.7.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.2",
    "openfin-cli": "^1.1.5",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.6",
    "webpack": "1.13.0",
    "webpack-dev-server": "1.14.1"
  }
}
